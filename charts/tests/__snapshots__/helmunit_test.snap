kind: Deployment
metadata:
  name: ingress-class-nginx-ingress-controller
  namespace: default
  labels:
    helm.sh/chart: nginx-ingress-2.3.0
    app.kubernetes.io/name: nginx-ingress
    app.kubernetes.io/instance: ingress-class
    app.kubernetes.io/version: "5.2.0"
